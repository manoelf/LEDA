import java.util.Scanner;

class StoogeSort {


	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		String[] lista = sc.nextLine().split(" ");
		sc.close();
		
		
		stoogeSort(lista);
		
		String saida = "";
		for (int i = 0; i < lista.length; i++) {
			saida += lista[i] + " ";
		}
		
		System.out.println(saida.substring(0, saida.length() - 1));
	}
	

	public static void stoogeSort(String[] A) {
		stoogeSort(A, 0, A.length - 1);
	}
	
	public static void stoogeSort(String[] A, int inicio, int fim) {
		if (Integer.parseInt(A[fim]) < Integer.parseInt(A[inicio])) 
			trocar(A, inicio, fim);
		
		if (inicio + 1 >= 	fim)
			return;
		
		int divisor = (fim - inicio + 1)/3;
		stoogeSort(A, inicio, fim - divisor);
		stoogeSort(A, inicio + divisor, fim);
		stoogeSort(A, inicio, fim - divisor);
	}
	
	public static void trocar(String[] A, int inicio, int fim) {
		String temp = A[inicio];
		A[inicio] = A[fim];
		A[fim] = temp;
	}

	

	
}
