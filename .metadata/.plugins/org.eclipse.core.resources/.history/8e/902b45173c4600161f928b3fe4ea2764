class StoogeSort {


	public static void main(String[] args) {
		
	}
	
	public static void swap(int[] A, int inicio, int fim) {
		int temp = A[inicio];
		A[inicio] = A[fim];
		A[fim] = temp;
	}

	
	public void stoogeSort(int[] A, int inicio, int fim) {
		if (A[fim] < A[inicio]) 
			swap(A, inicio, fim);
		
		if (inicio + 1 >= 	fim)
			return;
		int divisor = (fim - inicio + 1)/3;
		
		stoogeSort(A, inicio, fim - divisor);
		stoogeSort(A, inicio + divisor, fim);
		stoogeSort(A, inicio, fim - divisor);
	}
	
}