import java.util.Arrays;
import java.util.Scanner;

class StoogeSort {


	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		String[] lista = sc.nextLine().split(" ");
		sc.close();
		
		stoogeSort(lista, 0, lista.length - 1);
		
		Arrays.toString(lista);
		
	}
	
	public static void trocar(String[] A, int inicio, int fim) {
		String temp = A[inicio];
		A[inicio] = A[fim];
		A[fim] = temp;
	}

	
	public static void stoogeSort(String[] A, int inicio, int fim) {
		if (conv(A[fim]) < conv(A[inicio])) 
			trocar(A, inicio, fim);
		
		if (inicio + 1 >= 	fim)
			return;
		
		int divisor = (fim - inicio + 1)/3;
		stoogeSort(A, inicio, fim - divisor);
		stoogeSort(A, inicio + divisor, fim);
		stoogeSort(A, inicio, fim - divisor);
	}
	
	public static int conv(String valor) {
		int num = Integer.parseInt(valor);
		return num;
	}
	
}